/// @brief Page Initialization
///////////////////////////////////////////////////////////////////////////////
// ReturnPage=dp
DiagReturnPage=dp
if(HandleCallback==0)
{
  click luxCallback,0 //Clears callback vars
  click luxRunOnce,1
}else
{
  click luxCallback,1
}
click luxView,1
click luxAnimate,1
click luxController,1

///////////////////////////////////////////////////////////////////////////////
/// @fn luxCallback
/// @brief Control callbacks
///////////////////////////////////////////////////////////////////////////////
if(ReturnObj==0) //Direct to custom controller callback
{
  click luxController,0
}else
{
  click b[ReturnObj],0
}


